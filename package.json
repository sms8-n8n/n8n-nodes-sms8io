{
  "name": "n8n-nodes-sms8io",
  "version": "1.2.0",
  "description": "Official n8n community node for SMS8.io Android SMS Gateway - Send SMS, get message status, and manage devices",
  "keywords": [
    "n8n-community-node-package",
    "sms",
    "sms8",
    "sms8.io",
    "android",
    "gateway",
    "text-messages",
    "automation",
    "morocco",
    "mena",
    "bulk-sms",
    "mobile-messaging"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sms8-n8n/n8n-nodes-sms8io",
  "author": {
    "name": "SMS8 Team",
    "email": "support@sms8.io",
    "url": "https://sms8.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sms8-n8n/n8n-nodes-sms8io.git"
  },
  "bugs": {
    "url": "https://github.com/sms8-n8n/n8n-nodes-sms8io/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "format": "prettier --write nodes credentials",
    "test": "echo \"No tests yet\" && exit 0",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Sms8IoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Sms8Io/Sms8Io.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
